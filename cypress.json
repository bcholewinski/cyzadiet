{
    "baseUrl": "https://dietly.pl",
    "viewportWidth": 1600,
    "viewportHeight": 900,
    "defaultCommandTimeout": 10000,
    "pageLoadTimeout": 60000,
    "screenshotsFolder": "./cypress/snapshots/actual",
    "trashAssetsBeforeRuns": true,
    "env": {
        "failSilently": false
    },
    "blockHosts": [
        "*.googletagmanager.com",
        "*.google.com",
        "*.goadservices.com",
        "*.doubleclick.net",
        "*.facebook.net",
        "*onaudience.com",
        "*criteo.com",
        "*criteo.net",
        "*hotjar.com"
    ]
}